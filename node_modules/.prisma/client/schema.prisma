generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Booking {
  id        Int      @id @default(autoincrement())
  tripId    String
  seat      Int
  name      String
  phone     String
  email     String
  createdAt DateTime @default(now())
}
